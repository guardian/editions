        "@aws-cdk/aws-autoscaling-common": "^1.4.0",
        "@aws-cdk/aws-autoscaling-hooktargets": "^1.4.0",
        "@aws-cdk/aws-certificatemanager": "^1.4.0",
        "@aws-cdk/aws-cloudformation": "^1.4.0",
